##Railways
LAYER {
	11-20 {
        NAME: 'railways'
        TYPE: LINE
        @connection
        @layerconfig
		DATA {
			1-6: "geometry from (select geometry, osm_id, tunnel from osm_roads_gen0 where type='rail') as foo using unique osm_id using srid=3857"
            7-12: "geometry from (select geometry, osm_id, tunnel from osm_roads_gen1 where type='rail') as foo using unique osm_id using srid=3857"
            13-20: "geometry from (select geometry, osm_id, tunnel from osm_roads where type='rail') as foo using unique osm_id using srid=3857"
		}
		CLASS {
            EXPRESSION: ("[tunnel]"= '0')
            STYLE {
                WIDTH {
					1-10: 0.5
                    11-20: 1
				}
                COLOR {
					1-14: '#C0C0C0'
                    15-20: '#C0C0C0'
				}
            	LINECAP: BUTT
            }
            14-20 {
			STYLE {
                WIDTH {
					1-10: 0.5
                    11-20: 5
				}
                COLOR {
					1-14: '#777777'
                    15-20: '#777777'
				}
                PATTERN {{
					1 30
				}}
            	LINECAP: BUTT
            }
        	}
        }
    	CLASS {
        	STYLE {
                WIDTH {
					1-10: 0.5
                    11-20: 1
				}
                COLOR {
					1-14: '#777777'
                    15-20: '#777777'
				}
            	OPACITY: @tunnel_opacity
            	LINECAP: BUTT
            }
    		14-20 {
			STYLE {
                WIDTH {
					1-10: 0.5
                    11-20: 5
				}
                COLOR {
					1-14: '#777777'
                    15-20: '#777777'
				}
                PATTERN {{
					1 30
				}}
            	LINECAP: BUTT
            }}
        }
    }
}
##Roads
LAYER {
	5-20 {
        NAME: 'roads'
        TYPE: LINE
        @connection
        @layerconfig
		DATA {
	1-6: "geometry from (select osm_id,geometry,name,ref,type  from osm_roads_gen1 where type in ('motorway', 'trunk') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
        7: "geometry from (select osm_id,geometry,name,ref,type from osm_roads_gen1 where type in ('motorway', 'trunk', 'primary') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"            
        8: "geometry from (select osm_id,geometry,name,ref,type  from osm_roads_gen1 where type in ('motorway', 'trunk', 'primary') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
	9: "geometry from (select osm_id,geometry,name,ref,type  from osm_roads where type in ('motorway', 'motorway_link', 'trunk', 'trunk_link', 'primary', 'secondary', 'tertiary', 'residential', 'road', 'living_street', 'unclassified',  'track', 'footway', 'pedestrian') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
	10: "geometry from (select osm_id,geometry,name,ref,type from osm_roads where type in ('motorway', 'motorway_link', 'trunk', 'trunk_link', 'primary', 'secondary', 'tertiary', 'residential', 'road', 'living_street', 'unclassified', 'track', 'footway', 'pedestrian') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
	11-13: "geometry from (select osm_id,geometry,name, tunnel, ref,type from osm_roads where type in ('motorway', 'motorway_link', 'trunk', 'trunk_link', 'primary', 'secondary', 'tertiary', 'residential', 'road', 'living_street', 'unclassified','track', 'footway', 'pedestrian', 'ferry') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
	14-20: "geometry from (select oneway, osm_id,geometry,name,ref,type,tunnel,bridge from osm_roads where type in ('motorway', 'motorway_link', 'trunk', 'trunk_link', 'primary', 'primary_link', 'secondary', 'secondary_link', 'tertiary', 'tertiary_link', 'residential', 'road', 'living_street', 'unclassified', 'track', 'footway', 'pedestrian', 'service', 'ferry') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
		}
