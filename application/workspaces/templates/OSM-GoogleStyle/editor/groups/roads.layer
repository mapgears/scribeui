##Shields

LAYER {
 	7-20 {
     	NAME: 'shields'
        TYPE: ANNOTATION
        @connection
        @layerconfig
         DATA {
        	1-7: "geometry from (select osm_id,geometry,name, ref,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[1] as shield_class,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[2] as shield_no,type,tunnel,bridge from osm_roads_gen0 where type = 'motorway' order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857" 
		    8-20: "geometry from (select osm_id,geometry,name, ref,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[1] as shield_class,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[2] as shield_no,type,tunnel,bridge from osm_roads where type in ('motorway', 'trunk', 'primary') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
		}
        FILTER: (length('[shield_no]') != 0)
        LABELITEM: 'shield_no'
        CLASSITEM: 'shield_class'
        
        CLASS{
            EXPRESSION: "I"
            
            LABEL{
                @labelconfig_shields
                COLOR: @interstate_shield_lbl_clr
                SIZE: @interstate_shield_lbl_size
                STYLE{
                    SYMBOL: @interstate_shield_symbol
                    SIZE: @interstate_shield_size
                }
        	}
    
        }
        
        8-20 {
            CLASS{
                EXPRESSION: "US"
                    
                LABEL{
                    @labelconfig_shields
                    COLOR: @ushwy_shield_lbl_clr
                    SIZE: @ushwy_shield_lbl_size
                    STYLE{
                        SYMBOL: @ushwy_shield_symbol
                        SIZE: @ushwy_shield_size
                    }
                }
            }
        }
        
        13-20 {
            CLASS{
                EXPRESSION: "SR"
                
                LABEL{
                    @labelconfig_shields
                    COLOR: @secondary_shield_lbl_clr
                    SIZE: @secondary_shield_lbl_size
                    STYLE{
                        SYMBOL: @secondary_shield_symbol
                        SIZE: @secondary_shield_size
                    }
                }
            }
        }
    
	}
}


##Aeroways
LAYER {
	11-20 {
        NAME: 'aeroways'
        TYPE: LINE
        @connection
        @layerconfig
        DATA: 'geometry from (select geometry, osm_id, type from osm_aeroways) as foo using unique osm_id using srid=3857'
		CLASSITEM: 'type'
        CLASS {
			EXPRESSION: 'runway'
            STYLE {
				COLOR: 180 180 180
                WIDTH {
					1-11: 1
                    12: 2
                    13: 4
                    14: 9
                    15: 15
                    16: 30
                    17-20: 40
				}
            	LINECAP: BUTT
                LINEJOIN: MITER
			}
        	STYLE {
                COLOR: 80 80 80
               	WIDTH {
					1-15: 0
                    16-20: 1
				}
				PATTERN {{
					2 2
				}}
            	LINECAP: BUTT
			}
        	
		}
    	CLASS {
			EXPRESSION: 'taxiway'
            STYLE {
				COLOR: 180 180 180
                WIDTH {
					1-10: 0
                    11-12: 0.5
                    13: 1
                    14: 2
                    15: 4
                    16: 8
                    17-20: 12
				}
			}
		}
	}
}

7-20{
    LAYER {
            NAME: 'public_transport'
            TYPE: ANNOTATION
            @layerconfig
            @connection
            DATA {
                1-15: "geometry from (select ref, geometry, osm_id, type, name from osm_transport_points where type in ('aerodrome', 'station', 'motorway_junction')) as foo using unique osm_id using srid=3857"
                16-20: "geometry from (select ref, geometry, osm_id, type, name from osm_transport_points where type in ('bus_stop', 'station', 'motorway_junction', 'subway_entrance', 'terminal', 'tram_stop', 'aerodrome', 'helipad')) as foo using unique osm_id using srid=3857"
            }
            LABELITEM: 'name'
            CLASSITEM: 'type'
            
            CLASS {
                EXPRESSION: 'bus_stop'
                TEXT: ' '
                LABEL {
                    @labelconfig_public_transport
                    STYLE {
                        COLOR: 255 255 255
                        OUTLINECOLOR: 0 111 239
                        WIDTH: 0.5
                        SIZE: @public_transport_symbol_size
                        SYMBOL: @public_transport_symbol
                    }
                    STYLE {
                        COLOR: 0 111 239
                        SIZE: @public_transport_lbl_size
                        SYMBOL: 'bussymbol'
                    }
                }
            }
            
            CLASS {
                EXPRESSION: 'subway_entrance'
                TEXT: ' '
                LABEL {
                    @labelconfig_public_transport
                    STYLE {
                        COLOR: 0 111 239
                        OUTLINECOLOR: 255 255 255
                        WIDTH: 0.5
                        SIZE: @public_transport_symbol_size
                        SYMBOL: @public_transport_symbol
                    }
                    STYLE {
                        COLOR: 255 255 255
                        SIZE: @public_transport_lbl_size
                        SYMBOL: 'metrosymbol'
                    }
                }
            }
           
            CLASS {
                EXPRESSION: 'tram_stop'
                TEXT: ' '
                LABEL {
                    @labelconfig_public_transport
                    STYLE {
                        COLOR: 0 111 239
                        OUTLINECOLOR: 255 255 255
                        WIDTH: 0.5
                        SIZE: @public_transport_symbol_size
                        SYMBOL: @public_transport_symbol
                    }
                    STYLE {
                        COLOR: 255 255 255
                        SIZE: @public_transport_lbl_size
                        SYMBOL: 'tramwaysymbol'
                    }
                }
            }
           
            CLASS {
                EXPRESSION: ('[type]' = 'station')
                1-14 {
                    TEXT: ' '
                }
                LABEL {
                    @labelconfig_landusage
                    COLOR: '#3B5C79'
                    POSITION: UC
                    PRIORITY: 10
                    STYLE {
                        COLOR: 255 255 255
                        OUTLINECOLOR: 0 111 239
                        WIDTH: 0.5
                        SIZE: @public_transport_symbol_size
                        SYMBOL: @public_transport_symbol
                    }
                    STYLE {
                        COLOR: 0 111 239
                        SIZE: @public_transport_lbl_size
                        SYMBOL: 'stationsymbol'
                    }
                }
            }
            
            CLASS {
                EXPRESSION: ('[type]'='aerodrome' AND length('[name]') = 0)
                LABEL {
                    10-20 {
                        @labelconfig_landusage
                        COLOR: '#3F3931'
                        PRIORITY: 3
                        BUFFER: 5
                        POSITION: LC
                        OFFSET: @transport_offset
                        STYLE {
                            GEOMTRANSFORM: 'labelpnt'
                            OFFSET: @transport_offset
                            SYMBOL: 'aeroport'
                            COLOR: '#3F3931'
                            SIZE: 20
                        }
                    }
                }
            }
            
            CLASS {
                EXPRESSION: 'helipad'
                TEXT: 'H'
                LABEL {
                    1-20 {
                        @labelconfig
                        FONT: 'sc'
                        COLOR: 0 111 239
                        PRIORITY: 10
                        POSITION: CC
                        SIZE: 8
                        STYLE {
                            GEOMTRANSFORM: 'labelpnt'
                            SYMBOL: 'transportsquare'
                            SIZE {
                                1-16: 13
                                17-20: 15
                            }
                            COLOR: @transport_clr
                        }
                    }
                }
            }
            
        }
}
            
            